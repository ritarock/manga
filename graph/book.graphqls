interface Node {
    id: ID!
}

type Book implements Node {
    id: ID!
    isbn: String!
    title: String!
    publisher: String!
    pubdate: String!
    cover: String!
    author: String!
    subject_code: String!
}

type BookConnection implements Connection {
    pageInfo: PageInfo!
    edges: [BookEdge!]!
    nodes: [Book!]!
}

type BookEdge implements Edge {
    cursor: String!
    node: Book!
}
