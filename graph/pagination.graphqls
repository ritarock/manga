type PageInfo {
    startCursor: String
    endCursor: String
    hasNextPage: Boolean!
    hasPreviousPage: Boolean!
}

interface Connection {
    pageInfo: PageInfo!
    edges: [Edge!]!
    nodes: [Node!]!
}

interface Edge {
    cursor: String!
    node: Node!
}

input PaginationInput {
    first: Int
    after: String
    last: Int
    before: String
}
